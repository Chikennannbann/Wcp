package problem3;

public class Prob16 {
	public static void main(String[] args) {
		int num = 100;
		for (int i = 1; i <= num; i++) {
		if (num % 2 == 0) {
			System.out.println("2の倍数です");
		} else if (num % 3 == 0) {
			System.out.println("3の倍数です");
		} else if (num % (2 * 3) == 0) {
			System.out.println("2と3の公約数です");
		} else {
			System.out.println(num);
		}
		}
	}

}
