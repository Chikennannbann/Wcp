package chapter17;

import java.io.FileWriter;

public class Code5 {
	public static void main(String[] args) {
		FileWriter fw = null;
		try {
			fw = new FileWriter("data.txt");
			fw.write("hello");
		} catch (Exception e) {
			System.out.println("なんらかの例外が発生");
		} finally {
			fw.close();
		}
	}

}
