package Generics;

public class Apple02 extends Fruit {
	Apple02(String name) {
		super(name);
	}
	
	@Override
	protected String getName() {
		return name;
	}
	
	public static void main(String[] args) {
		Basket02<Apple02> appleBasket = new Basket02<>(new Apple02("ふじ"));
		appleBasket.replace(new Apple02("紅玉"));
		System.out.println(Apple02.appleBasket.getName());
	}

}
