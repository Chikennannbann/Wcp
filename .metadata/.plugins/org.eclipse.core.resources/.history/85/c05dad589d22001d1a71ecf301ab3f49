package problem3;

public class Prob16 {
	public static void main(String[] args) {
		int num = 60;
		if (num % (2 * 3) == 0) {
			System.out.println("公約数です");
		} else if (num % 3 == 0) {
			System.out.println("3の倍数です");
		} else if (num % 2 == 0) {
			System.out.println("2");
		} else {
			System.out.println(num);
		}
	}

}
