package extend.phone;

import extend.phone.Camera02;
import extend.phone.MobilePhone02;

public class Interface011 {
	public static void main(String[] args) {
		MobilePhone02 mobilePhone = new MobilePhone02("000-1111-2222");
		
		System.out.println("電話番号は最大" + MobilePhone02.MAX_NUMBER_DIGITS + "桁です");
		mobilePhone.call("99-8888-7777");
		Interface011.classTakePicture(mobilePhone);
	}
	
	public static void classTakePicture(Camera camera) {
		camera.takePicture();
	}

}
