package chapter5;

public class practice1 {
	public static void main(String[] args) {
		introduceOneself("ミナト");
		introduceOneself(20);
		introduceOneself(155.5);
		introduceOneself("子");
	}
	
	public static String introduceOneself(String name) {
		return name;
	}
	public static void introduceOneself(int age) {
		System.out.println("年齢は" + age);
	}
	public static void introduceOneself(double tall) {
		System.out.println("名前は" + tall);
	}
	public static void introduceOneself(char eto) {
		System.out.println("名前は" + eto);
	}

}
