package problem4;

public class Prob16 {
	public static void main(String[] args) {
		for (int i = 1; i <= 9; i++) {
			for (int j = 1; j <= 9; j++) {
				int m = i * j;
				if (i == 9 || j == 9) {
					System.out.println(i + "*" + j + "=" + m);
				} else if (i == 9) {
 				    System.out.println(i + "*" + j + "=" + m + " ");
				} else {
					System.out.print(i + "*" + j + "=" + m + " ");
				}
			}
		}
	}

}
