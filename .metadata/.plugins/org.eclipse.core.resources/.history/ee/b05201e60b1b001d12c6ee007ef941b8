package Generics;

public class Basket02<E extends Fruit> {
	private E elem;
	
	Basket02(E e) {
		elem = e;
	}
	
	void replace(E e) {
		elem = e;
	}
	
	E get() {
		return elem;
	}
	
	void printName() {
		Basket02<Fruit> appleBasket = new Baske02t<>(new Fruit("ふじ"));
		System.out.println("籠の中身は" + elem.getName() + "です");
	}

}
