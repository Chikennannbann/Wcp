package problem4;

public class practice {
	public static void main(String[] args) {
		int max = 1;
		int min = 100;
		for (int i = 1; i <= 5; i++) {
			int num = new java.util.Random().nextInt(100) + 1;
			System.out.println(num);
			
			if (max < num) {
				max = num;
			}
			if (min > num) {
				min = num;
			}
		}
		System.out.println("最大値" + max);
		System.out.println("最小値" + min);
	}

}

// 3 8 11 16
