package Generics;

public class Basket02<E extends Fruit> {
	public static void main(String[] args) {
	private E elem;
	
	Basket02(E e) {
		elem = e;
	}
	
	void replace(E e) {
		elem = e;
	}
	
	E get() {
		return elem;
	}
	
		void printName() {
			System.out.println("籠の中身は" + elem.getName() + "です");
		}
	}

}
