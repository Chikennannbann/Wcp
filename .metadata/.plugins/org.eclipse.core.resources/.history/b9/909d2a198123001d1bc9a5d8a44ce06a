package problem4;

public class practice {
	public static void main(String[] args) {
		int max = 1;
		int num = 1;
		for (int i = 1; i <= 5; i++) {
			num = new java.util.Random().nextInt(100) + 1;
			System.out.println(num);
		
		    if (max < num) {
			    max = num;
		    }
		}
		System.out.println("最大値" + max);
	}

}

